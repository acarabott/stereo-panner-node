{
  "name": "stereo-panner-node",
  "description": "StereoPannerNode for legacy Web Audio API",
  "version": "0.2.0",
  "author": "nao yonamine <mohayonao@gmail.com>",
  "bugs": {
    "url": "https://github.com/mohayonao/stereo-panner-node/issues"
  },
  "devDependencies": {
    "browserify": "^10.2.4",
    "eslint": "^0.24.0",
    "espower-loader": "^0.11.0",
    "intelli-espower-loader": "^0.7.0",
    "istanbul": "^0.3.17",
    "mocha": "^2.2.5",
    "power-assert": "^0.11.0",
    "uglify-js": "^2.4.23",
    "web-audio-test-api": "^0.3.4"
  },
  "files": [
    "lib",
    "package.json",
    "README.md"
  ],
  "homepage": "http://mohayonao.github.io/stereo-panner-node/",
  "keywords": [
    "webaudioapi",
    "stereopanner"
  ],
  "license": "MIT",
  "main": "lib/stereo-panner-node.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/mohayonao/stereo-panner-node.git"
  },
  "scripts": {
    "build": "npm run build-browser && npm run build-minify",
    "build-browser": "browserify lib/stereo-panner-node.js --standalone StereoPannerNode -o build/stereo-panner-node.js",
    "build-minify": "uglifyjs build/stereo-panner-node.js -o build/stereo-panner-node.min.js",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha",
    "lint": "eslint lib test",
    "test": "mocha",
    "travis": "npm run lint && npm run test"
  }
}
